<%- include header%>
<u1 class = "archive">
    <% var lastYear = 0 %>
    <% posts.forEach(function(post, index) { %>
        <% if (lastYear != post.time.year) { %>
            <li>
                <h3>
                    <%= post.time.year %>
                </h3>
            </li>
            <% lastYear = post.time.year } %>
        <li>
            <time>
                <%= post.time.year %>
            </time>
        </li>
        <li>
            <a href = "/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>">
                <%= post.title %>
            </a>
        </li>
    <% }) %>
</u1>
<%- include footer %>